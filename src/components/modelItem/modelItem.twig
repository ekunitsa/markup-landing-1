<div class="lan-modelItem">
  <div class="lan-modelItem_inner">
    <div class="lan-modelItem_img">
      <picture>
        <source srcset="./img/model/img{{ loop.index }}.avif 1x, ./img/model/img{{ loop.index }}@2x.avif 2x" type="image/avif">
        <source srcset="./img/model/img{{ loop.index }}.webp 1x, ./img/model/img{{ loop.index }}@2x.webp 2x" type="image/webp">
        <img class="lan-modelItem_imgSrc" src="./img/model/img{{ loop.index }}.jpg" alt="{{ item.title }}" srcset="./img/model/img{{ loop.index }}.jpg 1x, ./img/model/img{{ loop.index }}@2x.jpg 2x">
      </picture>
    </div>
    <div class="lan-modelItem_container">
      <div class="lan-modelItem_title">
        {{ item.title }}
      </div>
      <div class="lan-modelItem_text">
        {{ item.text }}
      </div>
      <div class="lan-modelItem_btnContainer">
        <a href="#callback" class="lan-modelItem_btn js-modal">Get Catalog</a>
      </div>
    </div>
  </div>
</div>
